    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Test with GCC",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/test",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Skip std",
                    "text": "-interpreter-exec console \"skip -rfu ^std::\"" // Skip everything in std
                }
            ],
            "preLaunchTask": "Build with Makefile",
            "miDebuggerPath": "/usr/bin/gdb"
        },
        {
            "name": "Debug examples with Makefile",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/examples/build/main",
            "args": [
                "-bool-1 ", " t",
                "-bool-2   ", "T",
                "-bool-3  ", " true",
                "  -bool-4", "  TRUE",
                "-bool-5 ", "True  ",
                "    -bool-6", "1",
                "-bool-7", "false",
                "-bool-8", "zxcasdasd",

                " -int32", "0004",
                "-int64  ", "  -13",
                "\t-uint32", "19 ",
                "   -uint64", "\t99asad",

                "-string", "  banicata   fsa  ",
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Skip std",
                    "text": "-interpreter-exec console \"skip -rfu ^std::\"" // Skip everything in std
                }
            ],
            "preLaunchTask": "Build examples with Makefile",
            "miDebuggerPath": "/usr/bin/gdb"
        },
    ]
}
